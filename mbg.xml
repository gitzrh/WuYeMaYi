<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

  <context id="DB2Tables" targetRuntime="MyBatis3">
  	
  	<!-- 抑制注释生成 -->
  	<commentGenerator>
	  <property name="suppressAllComments" value="true" />
	</commentGenerator>
  	
  	<!-- 配置数据库链接 -->
    <jdbcConnection 
    	driverClass="com.mysql.jdbc.Driver"
        connectionURL="jdbc:mysql:///wuyemy"
        userId="root"
        password="root">
    </jdbcConnection>

    <javaTypeResolver >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>
	
	<!-- 指定javaBean生成的位置 -->
    <javaModelGenerator targetPackage="com.wuyemy.bean" targetProject=".\src\main\java">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>
	
	<!-- 指定SQL映射文件生成的位置 -->
    <sqlMapGenerator targetPackage="mapper"  targetProject=".\src\main\resources">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>
	
	<!-- 指定mapper接口生成的位置 -->
    <javaClientGenerator type="XMLMAPPER" targetPackage="com.wuyemy.dao"  targetProject=".\src\main\java">
      <property name="enableSubPackages" value="true" />
    </javaClientGenerator>
	
	<!-- table指定表生成策略 -->
    <table tableName="biandong" domainObjectName="Biandong"></table>
    <table tableName="canshu" domainObjectName="Canshu"></table>
    <table tableName="jifen" domainObjectName="Jifen"></table>
    <table tableName="jinbilx" domainObjectName="Jinbilx"></table>
    <table tableName="kuser" domainObjectName="Kuser"></table>
    <table tableName="lunbotu" domainObjectName="Lunbotu"></table>
    <table tableName="manage" domainObjectName="Manage"></table>
    <table tableName="sdjilu" domainObjectName="Sdjilu"></table>
    <table tableName="tixian" domainObjectName="Tixian"></table>
    <table tableName="xiaozu" domainObjectName="Xiaozu"></table>
    <table tableName="yyzx" domainObjectName="Yyzx"></table>
    <table tableName="yyzxlvcanshu" domainObjectName="Yyzxlvcanshu"></table>
    <table tableName="yyzxsq" domainObjectName="Yyzxsq"></table>
    <table tableName="zhuangtai" domainObjectName="Zhuangtai"></table>
    <table tableName="zijinmingxi" domainObjectName="Zijinmingxi"></table>
    <table tableName="zixunguanli" domainObjectName="Zixunguanli"></table>
    
  </context>
</generatorConfiguration>